    def acao_opcao(self, opcao, bairro):
        if opcao == 'Auditiva':
            self.pagina_necessidade_auditiva(bairro)
        elif opcao == 'Visual':
            self.pagina_necessidade_visual(bairro)
        elif opcao == 'Fisica':
            self.pagina_necessidade_fisica(bairro)
        elif opcao == 'M.I.PS (Mental Intelectual e Psico Social)':
            self.pagina_necessidade_mips(bairro)
        elif opcao == 'Mudez':
            self.pagina_necessidade_mudez(bairro)




def main():
    root = tk.Tk()
    cade_sp = CadeSP(root)
    root.mainloop()

if __name__ == "__main__":
    main()
