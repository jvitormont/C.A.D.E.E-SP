import tkinter as tk

class CadeSP:
    def __init__(self, master):
        self.master = master
        self.master.title("C.A.D.E.E - SP")
       
        self.frame = tk.Frame(self.master)
        self.frame.pack()    
        self.label = tk.Label(self.frame, text="Centro de Apoio e Desenvolvimento para Educação Especial em São Paulo")
        self.label.pack()
        self.msg = tk.Label(self.frame, text="                                                                                                             ")
        self.msg.pack()
        self.subtitle = tk.Label(self.frame, text="Nossa meta consiste em prover para pessoas que necessitam assistência em suas limitações, permitindo-lhes localizar organizações próximas que possam oferecer a ajuda que buscam.")
        self.subtitle.pack()
        self.msg = tk.Label(self.frame, text="                                                                                                             ")
        self.msg.pack()
        self.subtitle = tk.Label(self.frame, text="Para um futuro inclusivo, que consiga prover suporte às famílias e indivíduos que requerem assistência em suas limitações, visando sua plena integração em nossa sociedade.")
        self.subtitle.pack()
        self.msg = tk.Label(self.frame, text="                                                                                                             ")
        self.msg.pack()

        self.button = tk.Button(self.frame, text="Prosseguir", command=self.pagina_zona)
        self.button.pack()
   
    def pagina_zona(self):
        self.frame.destroy()
        self.frame = tk.Frame(self.master)
        self.frame.pack()
       
        self.msg = tk.Label(self.frame, text="Qual Zona que deseja buscar auxílio?")
        self.msg.pack()
       
        self.zonas = [("Zona Sul", self.pagina_zona_sul),
                      ("Centro", self.pagina_zona_centro),
                      ("Zona Norte", self.pagina_zona_norte),
                      ("Zona Leste", self.pagina_zona_leste),
                      ("Zona Oeste", self.pagina_zona_oeste)]
       
        for zona, callback in self.zonas:
            tk.Button(self.frame, text=zona, command=callback).pack()

        # Botão Voltar
        tk.Button(self.frame, text="Voltar", command=self.voltar_para_inicio).pack()

    def voltar_para_inicio(self):
        self.frame.destroy()
        self.__init__(self.master)

    def mostrar_bairros(self, titulo, bairros, pagina_anterior):
        self.frame.destroy()
        self.frame = tk.Frame(self.master)
        self.frame.pack()

        self.msg = tk.Label(self.frame, text=titulo)
        self.msg.pack()

        for bairro in bairros:
            tk.Button(self.frame, text=bairro, command=lambda bairro=bairro: self.pagina_bairro(bairro, pagina_anterior)).pack()

        # Botão Voltar
        tk.Button(self.frame, text="Voltar", command=pagina_anterior).pack()

    def pagina_zona_sul(self):
        bairros = [
            "SACOMA", "IPIRANGA", "JABAQUARA", "MOEMA", "CURSINO",
            "GRAJAÚ", "VILA MARIANA", "CAMPO BELO", "CAMPO GRANDE",
            "CAMPO LIMPO", "CAPÃO REDONDO", "CIDADE ADEMAR",
            "CIDADE DUTRA", "SAÚDE", "JARDIM ÂNGELA", "JARDIM SÃO LUÍS",
            "SANTO AMARO", "VILA ANDRADE", "SOCORRO", "PARELHEIROS"
        ]
        self.mostrar_bairros("Bairros da Zona Sul:", bairros, self.pagina_zona)

    def pagina_zona_centro(self):
        bairros = [
            "BELA VISTA", "BOM RETIRO", "CAMBUCI", "CONSOLAÇÃO", "SÉ",
            "HIGIENÓPOLIS", "LIBERDADE", "REPUBLICA", "SANTA CECILIA/MARECHAL DEODORO", "ARARAQUARA"
        ]
        self.mostrar_bairros("Bairros do Centro:", bairros, self.pagina_zona)

    def pagina_zona_norte(self):
        bairros = [
            "JAÇANÃ", "MANDAQUI", "SANTANA", "TREMEMBÉ", "TUCURUVI", "VILA GUILHERME",
            "VILA MARIA", "VILA MEDEIROS", "BRASILANDIA", "VILA NOVA CACHOERINHA", "CASA VERDE",
            "FREGUESIA DO Ó", "JARAGUÁ", "LIMÃO"
        ]
        self.mostrar_bairros("Bairros da Zona Norte:", bairros, self.pagina_zona)

    def pagina_zona_leste(self):
        bairros = [
            "ALTO DE PINHEIROS","PINHEIROS","BARRA FUNDA","BUTANTÃ","ITAIM BIBI",
            "VILA JAGUARA","JAGUARÉ","JARDIM PAULISTA","LAPA","MORUMBI",
            "PERDIZES","RAPOSO TAVARES/COTIA","OSASCO","RIO PEQUENO",
            "VILA LEOPOLDINA","VILA SONIA","PRESIDENTE PRUDENTE"
        ]
        self.mostrar_bairros("Bairros da Zona Leste:", bairros, self.pagina_zona)

    def pagina_zona_oeste(self):
        bairros = [
            "ALTO DO TIETE/REGIÃO METROPOLITANA","AMERICANA","AGUA ROSA","ARICANDUVA","ARTUR ALVIM","BELENZINHO","BRÁS","CANGAIBA","VILA CARRÃO","MOOCA","PARI","PENHA","SÃO LUCAS","SOPEMBA","TATUAPÉ","VILA FORMOSA",
            "VILA MATILDE","VILA PRUDENTE","CIDADE LIDER","CIDADE TIRADENTES","GUAIANASES","IGUATEMI","ITAIM PAULISTA","ITAQUERA","JARDIM HELENA","JOSÉ BONIFACI","SÃO MATEUS","PARQUE SÃO RAFAEL","JUNDIAI","ERMELINO MATARAZZO",
            "GUARULHOS"
        ]
        self.mostrar_bairros("Bairros da Zona Oeste:", bairros, self.pagina_zona)

    def pagina_bairro(self, bairro, pagina_anterior):
        self.frame.destroy()
        self.frame = tk.Frame(self.master)
        self.frame.pack()

        self.msg = tk.Label(self.frame, text="Você selecionou o bairro: " + bairro)
        self.msg.pack()

        self.msg = tk.Label(self.frame, text="Escolha a necessidade que deseja buscar auxílio:")
        self.msg.pack()
       
        opcoes = ['Auditiva', 'Visual', 'Fisica', 'M.I.PS (Mental Intelectual e Psico Social)', 'Mudez']
        for opcao in opcoes:
            tk.Button(self.frame, text=opcao, command=lambda op=opcao: self.acao_opcao(op, bairro)).pack()

        # Botão Voltar
        tk.Button(self.frame, text="Voltar", command=pagina_anterior).pack()



